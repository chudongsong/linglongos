{
	"name": "@linglongos/api",
	"version": "1.0.0",
	"description": "",
	"private": true,
	"egg": {
		"typescript": true
	},
	"scripts": {
		"start": "eggctl start --daemon --title=egg-server-api --baseDir=./dist",
		"stop": "eggctl stop --title=egg-server-api",
		"dev": "egg-bin dev",
		"test:local": "egg-bin test",
		"pretest": "npm run clean && npm run lint -- --fix",
		"test": "egg-bin test",
		"preci": "npm run clean && npm run lint",
		"ci": "egg-bin cov",
		"postci": "npm run prepublishOnly && npm start && npm stop && npm run clean",
		"lint": "eslint . --cache",
		"tsc": "tsc",
		"clean": "tsc -b --clean && rm -rf dist",
		"export:openapi": "node ./scripts/export-openapi.js",
		"migrate:to-sqlite": "node ./scripts/run-migration.js",
		"test:storage": "npm run tsc && node ./scripts/test-storage.js",
		"prepublishOnly": "npm run clean && npm run tsc"
	},
	"dependencies": {
		"@eggjs/scripts": "^4.0.0",
		"@eggjs/tegg": "^3.5.2",
		"@eggjs/tegg-aop-plugin": "^3.5.2",
		"@eggjs/tegg-config": "^3.5.2",
		"@eggjs/tegg-controller-plugin": "^3.5.2",
		"@eggjs/tegg-eventbus-plugin": "^3.5.2",
		"@eggjs/tegg-plugin": "^3.5.2",
		"@eggjs/tegg-schedule-plugin": "^3.5.2",
		"@eggjs/tracer": "^3.0.0",
		"axios": "^1.7.7",
		"better-sqlite3": "^9.6.0",
		"egg": "^4.0.0",
		"egg-cors": "^3.0.1",
		"egg-swagger-doc": "^2.3.2",
		"egg-validate": "^2.0.2",
		"md5": "^2.3.0",
		"speakeasy": "^2.0.0",
		"sqlite3": "^5.1.7"
	},
	"devDependencies": {
		"@eggjs/bin": "7",
		"@eggjs/mock": "6",
		"@eggjs/tsconfig": "1",
		"@types/better-sqlite3": "^7.6.13",
		"@types/mocha": "10",
		"@types/node": "22",
		"eslint": "8",
		"eslint-config-egg": "14",
		"typescript": "5"
	},
	"engines": {
		"node": ">=20.18.1"
	},
	"repository": {
		"type": "git",
