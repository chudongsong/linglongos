{
	"name": "@linglongos/api-proxy",
	"version": "1.0.0",
	"description": "Node.js API proxy service for panel management",
	"main": "dist/index.js",
	"type": "module",
	"scripts": {
		"dev": "tsx watch src/index.ts",
		"build": "tsc && tsc-alias",
		"start": "node dist/index.js",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"lint": "eslint src/**/*.ts",
		"lint:fix": "eslint src/**/*.ts --fix",
		"clean": "rimraf dist"
	},
	"keywords": [
		"api-proxy",
		"panel-management",
		"1panel",
		"baota",
		"middleware"
	],
	"author": "LingLongOS Team",
	"license": "MIT",
	"dependencies": {
		"axios": "^1.12.2",
		"bcryptjs": "^3.0.2",
		"compression": "^1.8.1",
		"cors": "^2.8.5",
		"crypto-js": "^4.2.0",
		"dotenv": "^17.2.2",
		"express": "^5.1.0",
		"express-rate-limit": "^8.1.0",
		"helmet": "^8.1.0",
		"http-proxy-middleware": "^3.0.5",
		"joi": "^18.0.1",
		"jsonwebtoken": "^9.0.2",
		"node-cache": "^5.1.2",
		"sqlite": "^5.1.1",
		"sqlite3": "^5.1.7",
		"uuid": "^13.0.0",
		"winston": "^3.17.0",
		"winston-daily-rotate-file": "^5.0.0"
	},
	"devDependencies": {
		"@linglongos/eslint": "workspace:*",
		"@types/bcryptjs": "^3.0.0",
		"@types/compression": "^1.8.1",
		"@types/cors": "^2.8.19",
		"@types/crypto-js": "^4.2.2",
		"@types/express": "^5.0.3",
		"@types/jest": "^30.0.0",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/node": "^24.5.2",
		"@types/supertest": "^6.0.3",
		"@types/uuid": "^11.0.0",
		"jest": "^30.1.3",
		"rimraf": "^6.0.1",
		"supertest": "^7.1.4",
		"ts-jest": "^29.4.4",
		"tsc-alias": "^1.8.16",
		"tsx": "^4.20.5",
		"typescript": "^5.9.2"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
